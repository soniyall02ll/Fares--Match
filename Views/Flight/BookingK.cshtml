        <p>@(@flightSearch.From.Name)</p>*@

                                <input type="hidden" id="flight_from" value="@(Model.Flights.Outbound[0].FromAirport.Code)" />
                                <input type="hidden" id="flight_from_date" value="@(Model.FlightsSearch.DepDate)" />
                                <input type="hidden" id="flight_adult" value="@(Model.FlightsSearch.Adult)" />
                                <input type="hidden" id="flight_child" value="@(Model.FlightsSearch.Child)" />
                                <input type="hidden" id="flight_infant" value="@(Model.FlightsSearch.Infant)" />
                                <input type="hidden" id="flight_way" value="@(Model.FlightsSearch.FlightWay)" />
                                <input type="hidden" id="flight_class" value="@(@flightSearch.FlightClass)" />

                                <bdo>@(Model.Flights.Outbound[0].FromAirport.Code)</bdo>
                                <p>@(Model.Flights.Outbound[0].FromAirport.Name)</p>
                            </div>
                            <div class="duration">
                                <div class="arrowDiv"><div class="arrow"><i class="fa fa-angle-right"></i></div><div class="stps-one"><i class="fa fa-circle-o"></i></div></div>
                            </div>
                            <div class="resultCity">
                                <input type="hidden" id="flight_to" value="@(Model.FlightsSearch.To.Code)" />
                                <input type="hidden" id="flight_to_date" value="@(Model.FlightsSearch.RetDate)" />
                                <bdo>@(Model.Flights.Outbound[0].ToAirport.Code)</bdo>
                                <p>@(Model.Flights.Outbound[0].ToAirport.Name)</p>
                            </div>
                        </div>

                        @{ int cntOB = 1;
                            foreach (FlightsSegment outboundArr in Model.Flights.Outbound)
                            {

                                if (cntOB == 1)
                                {
                                    <div class="date-row">
                                        <span class="flightDlt-date">@(@Model.Flights.Outbound[0].DepartureDate.ToString("ddd dd MMMM yy"))</span>
                                        <span class="flightDlt-stop">
                                            <bdo>
                                                @*1 stop in DXB*@
                                                @((Model.Flights.Outbound.Count == 1) ? "Non" : (Model.Flights.Outbound.Count - 1) + "") stop(s) in @(Model.Flights.Outbound[0].ToAirport.Code)
                                            </bdo><bdo><i aria-hidden="true" class="fa fa-clock-o"></i> @(Math.Floor(double.Parse(Model.Flights.Outbound[Model.Flights.Outbound.Count - 1].TotalTime) / 60))h @(double.Parse(Model.Flights.Outbound[Model.Flights.Outbound.Count - 1].TotalTime) % 60)m</bdo>
                                        </span>
                                    </div>
                                }

                                // double stop_time = double.Parse(Common.GetTime(outboundArr.DepartureDate, outboundArr.ArrivalDate) + "");

                                double stop_time = 0;
                                if (Model.Flights.Outbound.Count > 1)
                                {
                                    stop_time = double.Parse(Common.GetTime(@Model.Flights.Outbound[Model.Flights.Outbound.Count - 1].DepartureDate, Model.Flights.Outbound[Model.Flights.Outbound.Count - 2].ArrivalDate) + "");
                                }

                                <div class="resultDetail-row">
                                    <div class="resultAirlines">
                                        <img src="@(ViewBag.StaticURL + "Content/airline/fit/" + outboundArr.Airline.Code + ".gif")" />
                                        <p>@(outboundArr.Airline.Name)</p>
                                        <span>@(outboundArr.CabinClass)</span>
                                    </div>
                                    <div class="resultCity">
                                        <bdo>@(outboundArr.FromAirport.Code)</bdo>
                                        <span>@(outboundArr.DepartureDate.ToString("HH:mm"))  </span>
                                        <p>@(outboundArr.DepartureDate.ToString("ddd dd-MMM-yyyy")) </p>
                                    </div>
                                    <div class="resultCity">
                                        <bdo>@(outboundArr.ToAirport.Code)</bdo>
                                        <span>@(outboundArr.ArrivalDate.ToString("HH:mm")) </span>
                                        <p>@(outboundArr.ArrivalDate.ToString("ddd dd-MMM-yyyy")) </p>
                                    </div>
                                </div>
                                <div class="flightDlt-baggage"><i class="fa fa-suitcase" aria-hidden="true"></i> @(outboundArr.BaggageAllowance),  Cabin: 7 KG, @(outboundArr.CabinClass)</div>


                                if (cntOB < Model.Flights.Outbound.Count)
                                {
                                    cntOB++;

                                    <div class="stopover-row"><i class="fa fa-hourglass-half"></i> Stop: @(Math.Floor(stop_time / 60))Hrs @(Math.Floor(stop_time % 60))Mins</div>

                                }
                            }
                        }
