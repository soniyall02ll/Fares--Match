
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section metatags{
    <title>Vuelos baratos, reservas, reserva de aerolíneas - FaresMatch</title>
    <link rel="canonical" href="https://www.faresmatch.com/es" />
    <meta name="keywords" content="Vuelo barato, Billete de avión, Reservas de vuelos, Vuelo de bajo coste, Reservas de entradas, billetes de avión más bajos" />
    <meta name="description" content="Encuentra los vuelos más baratos a cualquier destino. Compare precios, reserve billetes de avión con la agencia de viajes FaresMatch y ahorre dinero." />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="icon" type="image/png" sizes="96x96" href="https://www.faresmatch.com/images/favicon-96x96.png" />
    <meta name="robots" content="index, follow" />
}

<style>
    .grid1 {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 20px;
        img

    {
        width: 100%;
        border-radius: 10px;
    }

    .column {
        margin: auto 0;
    }

    }

    .newtexts {
        ol

    {
        margin-left: 20px;
    }

    ul {
        list-style-type: disc;
        margin-left: 20px;
    }

    }

    .grid2 {
        display: grid;
        grid-template-columns: repeat(3, 1fr);
        gap: 20px;
        img

    {
        width: 100%;
        border-radius: 10px;
    }

    .column {
        box-shadow: 0 0 2px 2px rgba(20, 20, 20, 0.1);
        padding: 15px;
        border-radius: 10px;
        h3

    {
        font-size: 23px !important;
    }

    }
    }

    @@media only screen and (max-width: 1024px) {
        .grid1,
        .grid2 {
            grid-template-columns: repeat(1, 1fr);
        }

            .grid2 .column h3 {
                font-size: 20px !important;
            }
    }
</style>

<div class="container2  bg1">
    <div class="row flex">
        <div class="column">
            <h1>Agencia de reserva de tiquetes de vuelos baratos</h1>
            @{Html.RenderPartial("~/Views/Shared/_SearchEngine.cshtml");}
        </div>
    </div>
</div>

<div class="container2 newtexts">
    <div class="row">
        <div class="column">
