                                            <p>@(inboundArr.DepartureDate.ToString("ddd dd-MMM-yyyy")) </p>
                                        </div>
                                        <div class="resultCity">
                                            <bdo>@(inboundArr.ToAirport.Code)</bdo>
                                            <span>@(inboundArr.ArrivalDate.ToString("HH:mm")) </span>
                                            <p>@(inboundArr.ArrivalDate.ToString("ddd dd-MMM-yyyy")) </p>
                                        </div>
                                    </div>
                                    <div class="flightDlt-baggage"><i class="fa fa-suitcase" aria-hidden="true"></i> @(inboundArr.BaggageAllowance),  Cabin: 7 KG, @(inboundArr.CabinClass)</div>


                                    if (cntIB < Model.Flights.Inbound.Count)
